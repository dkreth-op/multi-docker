{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "client",
        "image": "dk170589/multi-client-10-14",
        "hostname": "client",
        "essential": false,
        "memory": 128
      },
      {
        "name": "server",
        "image": "dk170589/multi-server-10-14",
        "hostname": "api",
        "essential": false,
        "memory": 128
      },
      {
        "name": "worker",
        "image": "dk170589/multi-worker-10-14",
        "hostname": "worker",
        "essential": false,
        "memory": 128
      },
      {
        "name": "nginx",
        "image": "dk170589/multi-nginx-10-14",
        "hostname": "nginx",
        "essential": true,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 80
          }
        ],
        "links": ["client", "server"],
        "memory": 128
      }
    ]
  }
  